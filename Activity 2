#include <iostream>
#include <algorithm>
    using namespace std;
    
    int main()
    {
        int randomArry[10];
        for (int i = 0; i < 10; i++)
        {
            //rand is use ro generate a random  variable
            //using 50% to make sure number aren't too big
            randomArray[i] = rand() % 50;
            cout << randomArray[i] << endl;

        }
        int n[1000];
        for (int i = 0; i < 1000; i++)
        {
            n[i] = rand() % 100;
            cout << n[i] << endl;
        }
        int counter = 0;
        for (int i = 0; i < 1000; i++)
        {
            if (n[i] == 6)
            {
                counter++;
            }
        }
        cout << "The  number 6 appeared" << counter << "times" << endl;
        return 0;
    }
